<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainWindowController">
   <children>
      <TextArea fx:id="txtarea_output" editable="false" prefHeight="600.0" prefWidth="465.0" text="&lt;Empty&gt;" wrapText="true" />
      <Button fx:id="btn_lightcycle" layoutX="478.0" layoutY="21.0" mnemonicParsing="false" onAction="#cyclelights" prefHeight="25.0" prefWidth="305.0" text="Begin Cycle" />
      <Button fx:id="btn_allon" layoutX="478.0" layoutY="85.0" mnemonicParsing="false" onAction="#allLightsOn" text="All On" />
      <Button fx:id="btn_alloff" layoutX="731.0" layoutY="85.0" mnemonicParsing="false" onAction="#allLightsOff" text="All Off" />
      <TextField fx:id="txt_Lcd" layoutX="482.0" layoutY="195.0" />
      <Button fx:id="btn_setLcd" layoutX="641.0" layoutY="195.0" mnemonicParsing="false" onAction="#setLcd" text="Button" />
      <Slider fx:id="slider_lightselect" blockIncrement="1.0" layoutX="478.0" layoutY="138.0" majorTickUnit="2.0" max="34.0" minorTickCount="1" prefHeight="14.0" prefWidth="305.0" showTickMarks="true" />
      <Button fx:id="btn_getVolume" layoutX="477.0" layoutY="288.0" mnemonicParsing="false" onAction="#getVolume" text="GetVolume" />
      <Label fx:id="lbl_volume" layoutX="567.0" layoutY="292.0" prefHeight="17.0" prefWidth="130.0" text="Label" />
      <ProgressBar fx:id="prog_volume" layoutX="493.0" layoutY="353.0" prefWidth="200.0" progress="0.0" />
      <Label fx:id="lbl_volume" layoutX="703.0" layoutY="354.0" prefHeight="17.0" prefWidth="76.0" text="Label" />
      <CheckBox fx:id="chk_playOnce" layoutX="704.0" layoutY="199.0" mnemonicParsing="false" text="Play Once" />
   </children>
</AnchorPane>
